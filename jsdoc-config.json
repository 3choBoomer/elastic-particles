{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
      "include": ["lib", "README.md"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "footer": "",
    "copyright": "",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "html://",
    "systemName": "Elastic Particles",
    "theme": "lumen",
    "syntaxTheme": ""
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs",
    "template": "./node_modules/jsdoc-simple-template"
  }
}